{
    "name": "agungsugiarto/legacy-to-laravel",
    "type": "library",
    "description": "Help upgrade from CodeIgniter3 to Laravel",
    "keywords": ["codeigniter","codeigniter3", "laravel", "laravel-framework"],
    "homepage": "https://github.com/agungsugiarto/legacy-to-laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "Kenji Suzuki",
            "homepage": "https://github.com/kenjis"
        },
        {
            "name": "Agung Sugiarto",
            "homepage": "https://github.com/agungsugiarto"
        }
    ],
    "require": {
        "php": "^7.3 || ^8.0"
    },
    "require-dev": {
        "mikey179/vfsstream": "^1.6.10",
        "laravel/framework": "^8.83",
        "friendsofphp/php-cs-fixer": "^3.9"
    },
    "autoload": {
        "psr-4": {
            "Fluent\\Legacy\\": "src/"
        },
        "files": [
            "src/bootstrap.php"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.x-dev"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
